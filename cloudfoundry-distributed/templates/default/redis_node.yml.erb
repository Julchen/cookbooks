---
local_db: sqlite3:/var/vcap/services/redis/redis_node.db
mbus: nats://<%= @mbus_ip %>:4222
base_dir: /var/vcap/services/redis/instances
ip_route: <%= @host %>
log_level: DEBUG
pid: /var/vcap/sys/run/redis_node.pid
available_memory: 4096
max_memory: 16
max_swap: 32
node_id: <%= @node_id %> 
redis_path: redis-server
port_range:
  first: 5000
  last: 25000
